<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <SelectDialog
        id="ocProductValueHelp"
        noDataText="Loading..."
        title="Select Material No"
        search="onValueHelpSearch"
        liveChange="onValueHelpSearch"
        confirm="onOCValueConfirmItem"
        cancel="onValueHelpClosevoy"
        items="{uniqueProductModel>/products}"
        initialFocus="SearchField">
        <StandardListItem 
            id="ocProductValueHelp_StandardListItem"
            title="{uniqueProductModel>BatchId}"
            description=""
            iconDensityAware="false"
            iconInset="false"
            type="Active" />
    </SelectDialog>
</core:FragmentDefinition>